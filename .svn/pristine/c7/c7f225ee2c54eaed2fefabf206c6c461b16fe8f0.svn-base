using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BLL
{
    class Message
    {
        private string _id;

        private string _name;

        private string _sex;

        private string _college;

        private string _grade;

        private string _email;

        private string _phone;

        private string _major;

        private string _class;

        private string _idCard;

        private string _introduction;

        private string _groupNumber;

        public string Id
        {
            get { return _id; }
            set { _id = value; }
        }
        public string Name
        {
            get { return _name; }
            set { _name = value; }
        }
        public string Sex
        {
            get { return _sex; }
            set { _sex = value; }
        }
        public string College
        {
            get { return _college; }
            set { _college = value; }
        }
