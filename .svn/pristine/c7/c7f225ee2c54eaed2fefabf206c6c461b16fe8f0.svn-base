        {
            get { return _email; }
            set { _email = value; }
        }
        public string Phone
        {
            get { return _phone; }
            set { _phone = value; }
        }
        public string Major
        {
            get { return _major; }
            set { _major = value; }
        }
        public string Class
        {
            get { return _class; }
            set { _class = value; }
        }
        public string IdCard
        {
            get { return _idCard; }
            set { _idCard = value; }
